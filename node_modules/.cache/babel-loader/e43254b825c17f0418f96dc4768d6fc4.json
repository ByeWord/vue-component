data() {\r\n      return {\r\n        gutter: 0\r\n      }\r\n    },\r\n    computed: {\r\n      colStyle() {\r\n        let {gutter} = this;\r\n        return !!gutter ? {paddingLeft: `${gutter / 2}px`, paddingRight: `${gutter / 2}px`} : ''\r\n      },\r\n      colClass() {\r\n        let {span, offset, phone, pad, narrowPc, pc, widePc} = this;\r\n\r\n        let createClasses = (data,str=\"\")=>{\r\n          if (!data)return[];\r\n          let array = [];\r\n          if (data.span){\r\n            array.push(`w-col-${str}${data.span}`)\r\n          }\r\n          if (data.offset){\r\n            array.push(`w-col-offset-${str}${data.offset}`)\r\n          }\r\n        };\r\n\r\n        return [\r\n          ...(span ? [`w-col-${span}`] : [`w-col-24`]),\r\n          offset && `w-col-offset-${offset}`,\r\n          ...(phone ? [`w-col-phone-${phone.span}`] : []),\r\n          ...(pad ? [`w-col-pad-${pad.span}`] : []),\r\n          ...(narrowPc ? [`w-col-narrow-pc-${narrowPc.span}`] : []),\r\n          ...(pc ? [`w-col-pc-${pc.span}`] : []),\r\n          ...(widePc ? [`w-col-wide-pc-${widePc.span}`] : [])\r\n        ]\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n    $colPrefix: w-col;\r\n    $offsetPrefix: w-col-offset;\r\n    .w-col {\r\n        float: left;\r\n        box-sizing: border-box;\r\n    }\r\n\r\n    @for $i from 1 through 24 {\r\n        .#{$colPrefix}-#{$i} {\r\n            width: 100%*$i/24;\r\n        }\r\n        .#{$offsetPrefix}-#{$i} {\r\n            margin-left: 100%*$i/24;\r\n        }\r\n    }\r\n\r\n    /*wide-pc*/\r\n    @media only screen and (min-width: 1201px) {\r\n        @for $i from 1 through 24 {\r\n            .#{$colPrefix}-wide-pc-#{$i} {\r\n                width: 100%*$i/24;\r\n            }\r\n        }\r\n    }\r\n\r\n    /*pc*/\r\n    @media only screen and (max-width: 1200px) {\r\n        @for $i from 1 through 24 {\r\n            .#{$colPrefix}-pc-#{$i} {\r\n                width: 100%*$i/24;\r\n            }\r\n        }\r\n    }\r\n\r\n    /*narrow-pc*/\r\n    @media only screen and (max-width: 993px) {\r\n        @for $i from 1 through 24 {\r\n            .#{$colPrefix}-narrow-pc-#{$i} {\r\n                width: 100%*$i/24;\r\n            }\r\n        }\r\n    }\r\n\r\n    /*pad*/\r\n    @media only screen and (max-width: 769px) {\r\n        @for $i from 1 through 24 {\r\n            .#{$colPrefix}-pad-#{$i} {\r\n                width: 100%*$i/24;\r\n            }\r\n        }\r\n    }\r\n\r\n    /*phone*/\r\n    @media only screen and (max-width: 576px) {\r\n        @for $i from 1 through 24 {\r\n            .#{$colPrefix}-phone-#{$i} {\r\n                width: 100%*$i/24;\r\n            }\r\n        }\r\n    }\r\n\r\n</style>"],"sourceRoot":"src/components"}]}